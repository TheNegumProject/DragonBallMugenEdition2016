
[Info]
name = "Piccolo_EB"        ;Name of character
displayname = "Piccolo" ;Name of character to display
versiondate = 08,15,2015    ;Version date of character (MM-DD-YYYY or X.XX)
mugenversion = 1.0          ;Version of M.U.G.E.N character works on (X.XX)
author = "Ironjw"         ;Character author name
pal.defaults = 1      ;Default palettes in order of preference (up to 4)
localcoord = 320,240        ;Local coordinate space width and height


[Files]
cmd     = Piccolo_EB.cmd           ;Command set
cns     = Piccolo_EB.cns           ;Constants
st      = Piccolo_EB.cns           ;States
stcommon = common1.cns      ;Common states (from data/ or motif)
sprite  = Piccolo_EB.sff           ;Sprite
anim    = Piccolo_EB.air           ;Animation
sound   = Piccolo_EB.snd           ;Sound
ai      = Piccolo_EB.ai            ;AI hints data (not used)

[Palette Keymap]
x = 1 ;Press button X to select palette 1, etc.
y = 2
z = 3
a = 4
b = 5
c = 6

; Arcade mode
[Arcade]
intro.storyboard = intro.def
ending.storyboard = ending.def
